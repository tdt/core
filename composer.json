{
    "name": "tdt/core",
    "description": "A RESTful data adapter",
    "keywords": ["data adapter", "data transformation", "REST API"],
    "homepage": "http://thedatatank.com",
    "type": "library",
    "license": "AGPLv3",
    "suggest": {
        "tdt/start": "Maps the functions of this scheduler to a RESTful API. A controller is already included: tdt scheduler ScheduleController"
    },
    "require": {
        "gabordemooij/redbean": ">=3.2" ,
        "tdt/exceptions" : "dev-master",
        "tdt/negotiators" : "dev-master",
        "tdt/cache" : "dev-master",
        "monolog/monolog": "1.2.*",
        "justinrainbow/json-schema": "dev-master",
        "CodePlex/PHPExcel": "dev-master",
        "tdt/formatters" : "dev-master"

    },
    "minimum-stability" : "dev",
    "autoload": {
        "psr-0": {
            "tdt\\core\\": "src/"
        }
    }
}